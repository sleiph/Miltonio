    <string name="sem4_poo_p3r5">jdk -version</string>
    <string name="sem4_poo_p3r6">jdk</string>

    <string name="sem4_poo_p4">Como indicar no Java que uma <b>ClasseFilha</b> herdará de alguma outra <b>Classe</b>?</string>
    <string name="sem4_poo_p4r1">public class ClasseFilha <b>extends</b> Classe {}</string>
    <string name="sem4_poo_p4r2">public class ClasseFilha <b>from</b> Classe {}</string>
    <string name="sem4_poo_p4r3">public class ClasseFilha <b>:</b> Classe {}</string>
    <string name="sem4_poo_p4r4">public class ClasseFilha <b>=></b> Classe {}</string>
    
